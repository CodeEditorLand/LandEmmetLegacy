{
	"name": "emmet",
	"displayName": "emmet",
	"version": "0.0.1",
	"private": false,
	"description": "Emmet support for VS Code",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandEmmetLegacy#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandEmmetLegacy/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandEmmetLegacy.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/src/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -watch -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"emmet.suggestAbbreviations": {
					"default": true,
					"description": "Shows possible emmet abbreviations as suggestions",
					"type": "boolean"
				},
				"emmet.suggestExpandedAbbreviation": {
					"default": true,
					"description": "Shows expanded emmet abbreviations as suggestions",
					"type": "boolean"
				}
			},
			"title": "Emmet configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"onLanguage:html",
		"onLanguage:jade",
		"onLanguage:slim",
		"onLanguage:haml",
		"onLanguage:xml",
		"onLanguage:xsl",
		"onLanguage:css",
		"onLanguage:scss",
		"onLanguage:sass",
		"onLanguage:less",
		"onLanguage:stylus",
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact"
	],
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.10"
	},
	"publishConfig": {
		"access": "public"
	}
}
