{
  "name": "emmet",
  "displayName": "emmet",
  "version": "0.0.1",
  "description": "Emmet support for VS Code",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-emmet"
  },
  "publisher": "vscode",
  "main": "./out/src/extension",
  "scripts": {
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "tsc -p ./"
  },
  "contributes": {
    "configuration": {
      "properties": {
        "emmet.suggestAbbreviations": {
          "default": true,
          "description": "Shows possible emmet abbreviations as suggestions",
          "type": "boolean"
        },
        "emmet.suggestExpandedAbbreviation": {
          "default": true,
          "description": "Shows expanded emmet abbreviations as suggestions",
          "type": "boolean"
        }
      },
      "title": "Emmet configuration",
      "type": "object"
    }
  },
  "activationEvents": [
    "onLanguage:html",
    "onLanguage:jade",
    "onLanguage:slim",
    "onLanguage:haml",
    "onLanguage:xml",
    "onLanguage:xsl",
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:sass",
    "onLanguage:less",
    "onLanguage:stylus",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "dependencies": {
    "@emmetio/css-parser": "^0.3.0",
    "@emmetio/expand-abbreviation": "^0.5.4",
    "@emmetio/extract-abbreviation": "^0.1.1",
    "@emmetio/html-matcher": "^0.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "typescript": "^2.0.3",
    "vscode": "^1.0.0"
  }
}
